    <normal bg_color="#282828"/>
    <pressed bg_color="#191919"/>
    <over bg_color="#f0f0f0" text_color="black"/>
  </style>
  <style border="bottom" border_color="#313131" name="odd_clickable" text_color="#BBBBBB">
    <normal bg_color="#282828"/>
    <pressed bg_color="#191919"/>
    <over bg_color="#484848"/>
  </style>
  <style border="bottom" border_color="#313131" name="even_clickable" text_color="#BBBBBB">
    <normal bg_color="#282828"/>
    <pressed bg_color="#191919"/>
    <over bg_color="#484848"/>
  </style>
  <style border="bottom" border_color="#a0a0a0" name="odd" text_color="black">
