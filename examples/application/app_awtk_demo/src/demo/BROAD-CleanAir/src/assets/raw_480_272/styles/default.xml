</scroll_bar>

<dragger>
  <style name="scroll_bar">
    <normal bg_color="#d0d0d0"/>
    <pressed bg_color="#c0c0c0"/>
    <over bg_color="#c0c0c0"/>
  </style>
</dragger>

<list_item>
