<dialog anim_hint="center_scale" h="115" w="347" x="c" y="m">
	<dialog_title h="20" text="定时设置" w="100%" x="0" y="0"/>
	<dialog_client h="-20" w="100%" x="0" y="bottom">
		<row h="17" layout="row:1 col:3" w="120" x="17" y="6">
			<label style="timing" text="时"/>
			<label style="timing" text="分"/>
			<label style="timing" text="秒"/>
		</row>
		<row h="68" layout="row:1 col:3" w="120" x="18" y="23">
			<text_selector name="hour_selector" options="1-24" text="15" visible_nr="3"/>
			<text_selector name="min_selector"  options="1-60" text="10" visible_nr="3"/>
			<text_selector name="sec_selector"  options="1-60" text="16" visible_nr="3"/>
		</row>
		<button h="20" name="ok" style="timing_switch_btn" text="开" w="90" x="right:18" y="18"/>
		<button h="20" name="cancle" style="timing_switch_btn" text="关" w="90" x="right:18" y="bottom:18"/>
	</dialog_client>
</dialog>