  <style border="bottom" border_color="#a0a0a0" name="even" text_color="black">
    <normal bg_color="#eaeaea"/>
    <pressed bg_color="#eaeaea"/>
    <over bg_color="#eaeaea"/>
  </style>
  <style name="empty">
    <normal/>
    <pressed/>
    <over/>
  </style>
